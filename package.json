{
  "name": "yang-js",
  "version": "0.16.18",
  "description": "YANG parser and composer",
  "keywords": [
    "yang",
    "compile",
    "compose",
    "parse",
    "expression",
    "require",
    "model",
    "schema",
    "adaptive",
    "validate",
    "object",
    "rfc6020"
  ],
  "author": "Peter Lee <peter@corenova.com>",
  "homepage": "https://github.com/corenova/yang-js",
  "license": "Apache-2.0",
  "repository": "corenova/yang-js",
  "main": "lib/main.js",
  "models": {
    "iana-crypt-hash": "./schema/iana-crypt-hash.yang",
    "ietf-yang-types": "./schema/ietf-yang-types.yang",
    "ietf-inet-types": "./schema/ietf-inet-types.yang",
    "ietf-yang-library": "./lib/module/ietf-yang-library.js",
    "yang-meta-types": "./schema/yang-meta-types.yang"
  },
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "delegates": "^1.0.0",
    "indent-string": "^2.1.0",
    "promise": "^7.1.1",
    "stacktrace-parser": "^0.1.4",
    "yang-parser": "^0.1.0"
  },
  "bundleDependencies": [
    "indent-string",
    "promise",
    "yang-parser"
  ],
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "coffee-script": ">=1.7.0",
    "mocha": "~2.0.1",
    "rimraf": "^2.5.2",
    "should": "~3.1.3",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyify": "^3.0.3"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/* lib/*",
    "prebuild": "npm run clean -s && mkdir -p dist",
    "build:src": "coffee -o lib -c src",
    "build:full": "browserify -t brfs -i crypto -i buffer -r promise/polyfill.js lib/main.js > dist/yang.js",
    "build:min": "browserify -t brfs -p [ minifyify --no-map ] -i crypto -i buffer -r promise/polyfill.js lib/main.js > dist/yang.min.js",
    "build": "npm run build:src && npm run build:full && npm run build:min",
    "prepublish": "npm run build",
    "pretest": "npm run build:src",
    "test": "mocha"
  }
}
